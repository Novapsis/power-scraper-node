{
  "name": "@novapsis/n8n-nodes-novapsis",
  "version": "0.4.2",
  "description": "Un nodo para n8n que se conecta al microservicio Power Scraper para extraer el HTML de cualquier p√°gina web.",
  "keywords": [
    "n8n-community-node-package",
    "scraper",
    "novapsis",
    "web scraping"
  ],
  "license": "MIT",
  "homepage": "https://www.novapsis.com",
  "author": {
    "name": "Antonio Lopez",
    "email": "novapsiscorp@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Novapsis/power-scraper-node.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write nodes credentials",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
